{
  "name": "typescript-angular-1.5",
  "language": {
    "name": "typescript",
    "defaultExtension": ".ts"
  },
  "dependencies": {},
  "definitionFilters": [
    "arrayName",
    "pascalCaseName"
  ],
  "operationFilters": [
    "camlCaseName",
    "camlCaseArgumentName"
  ],
  "modes": {
    "singleFile": {
      "entries": [
        {
          "template": "singleFile.hbs",
          "naming": "{{options.clientName}}.ts",
          "selector": ""
        }
      ]
    },
    "multipleFiles": {
      "entries": [
        {
          "template": "client.hbs",
          "naming": "{{options.clientName}}.cs",
          "selector": ""
        },
        {
          "template": "definition.hbs",
          "naming": "{{data.name}}.cs",
          "selector": "definitions"
        }
      ]
    }
  }
}