{
  "name": "typescript-angular-1.5",
  "language": {
    "name": "typescript",
    "defaultExtension": ".ts"
  },
  "dependencies": {},
  "definitionFilters": [
    "arrayName",
    "pascalCaseName"
  ],
  "operationFilters": [
    "camlCaseName",
    "camlCaseArgumentName"
  ],
  "modes": {
    "singleFile": {
      "entries": [
        {
          "template": "clientConfigurationService.hbs",
          "naming": "clientConfigurationService.ts",
          "selector": ""
        },
        {
          "template": "singleFile.hbs",
          "naming": "{{options.clientName}}.ts",
          "selector": ""
        }
      ]
    },
    "multipleFiles": {
      "entries": [
        {
          "template": "multipleFilesClient.hbs",
          "naming": "{{options.clientName}}.ts",
          "selector": ""
        },
        {
          "template": "multipleFilesInterface.hbs",
          "naming": "I{{options.clientName}}.d.ts",
          "selector": ""
        }
      ]
    },
    "package": {
      "entries": [
        {
          "template": "clientConfigurationService.hbs",
          "naming": "ClientConfigurationService.ts",
          "selector": ""
        },
        {
          "template": "packageClient.hbs",
          "naming": "{{options.clientName}}.ts",
          "selector": ""
        },
        {
          "template": "package.hbs",
          "naming": "package.json",
          "selector": ""
        }
      ]
    }
  }
}
