{
  "name": "csharp-netstandard-1.3",
  "language": {
    "name": "csharp",
    "defaultExtension": ".cs"
  },
  "dependencies": {
    "NETStandard.Library": {
      "version": "1.6.0"
    },
    "Newtonsoft.Json": {
      "version": "9.0.1"
    },
    "Microsoft.Extensions.Options": {
      "version": "1.0.0"
    },
    "GeekLearning.RestKit.Core": {
      "version": "0.1.1-*"
    },
    "GeekLearning.RestKit.Json": {
      "version": "0.1.1-*"
    }
  },
  "definitionFilters": [
    "arrayName",
    "pascalCaseName"
  ],
  "operationFilters": [
    "pascalCaseName",
    "pascalCaseArgumentName",
    "optionalArgsOrder"
  ],
  "modes": {
    "project": {
      "entries": [
        {
          "template": "project.hbs",
          "naming": "project.json",
          "selector": ""
        },
        {
          "template": "xproj.hbs",
          "naming": "{{options.clientName}}.xproj",
          "selector": ""
        },
        {
          "template": "client.hbs",
          "naming": "{{options.clientName}}.cs",
          "selector": ""
        },
        {
          "template": "clientOptions.hbs",
          "naming": "{{options.clientName}}Options.cs",
          "selector": ""
        },
         {
          "template": "clientExtensions.hbs",
          "naming": "{{options.clientName}}Extensions.cs",
          "selector": ""
        },
        {
          "template": "definition.hbs",
          "naming": "{{data.name}}.cs",
          "selector": "definitions"
        }
      ]
    }
  }
}