{
  "name": "typescript-angular-2.0",
  "language": {
    "name": "typescript",
    "defaultExtension": ".ts"
  },
  "dependencies": {},
  "definitionFilters": [
    "arrayName",
    "pascalCaseName"
  ],
  "operationFilters": [
    "camlCaseName",
    "camlCaseArgumentName"
  ],
  "modes": {
    "singleFile": {
      "entries": [
        {
          "template": "singleFile.hbs",
          "naming": "{{options.clientName}}.ts",
          "selector": ""
        }
      ]
    },
    "package": {
      "entries": [
        {
          "template": "package.hbs",
          "naming": "package.json",
          "selector": ""
        },
        {
          "template": "tsconfig.hbs",
          "naming": "tsconfig.json",
          "selector": ""
        },
        {
          "template": "angular-cli.hbs",
          "naming": ".angular-cli.json",
          "selector": ""
        },
        {
          "template": "ng-package.hbs",
          "naming": "ng-package.json",
          "selector": ""
        },
        {
          "template": "singleFile.hbs",
          "naming": "src/{{options.packageName}}/{{options.clientName}}.ts",
          "selector": ""
        },
        {
          "template": "lib-package.hbs",
          "naming": "src/{{options.packageName}}/package.json",
          "selector": ""
        }
      ]
    }
  }
}