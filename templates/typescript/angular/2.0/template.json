{
  "name": "typescript-angular-2.0",
  "language": {
    "name": "typescript",
    "defaultExtension": ".ts"
  },
  "dependencies": {},
  "definitionFilters": [
    "arrayName",
    "pascalCaseName"
  ],
  "operationFilters": [
    "camlCaseName",
    "camlCaseArgumentName"
  ],
  "modes": {
    "singleFile": {
      "entries": [
        {
          "template": "singleFile.hbs",
          "naming": "{{options.clientName}}.ts",
          "selector": ""
        }
      ]
    },
    "package": {
      "entries": [
        {
          "template": "package.hbs",
          "naming": "package.json",
          "selector": ""
        },
        {
          "template": "singleFile.hbs",
          "naming": "{{options.clientName}}.ts",
          "selector": ""
        }
      ]
    }
  }
}