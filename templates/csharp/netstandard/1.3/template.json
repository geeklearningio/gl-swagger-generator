{
    "name": "csharp-netstandard-1.3",
    "language": {
        "name": "csharp",
        "defaultExtension": ".cs"
    },
    "dependencies": {
        "NETStandard.Library": {
            "version": "1.6.1"
        },
        "Newtonsoft.Json": {
            "version": "9.0.1"
        },
        "System.Runtime.Extensions": {
            "version": "4.3.0"
        },
        "Microsoft.Extensions.Options": {
            "version": "1.1.2"
        },
        "GeekLearning.RestKit.Core": {
            "version": "1.2.0-*"
        },
        "GeekLearning.RestKit.Json": {
            "version": "1.2.0-*"
        },
        "GeekLearning.RestKit.FormData": {
            "version": "1.2.0-*"
        }
    },
    "definitionFilters": [
        "arrayName",
        "pascalCaseName"
    ],
    "operationFilters": [
        "pascalCaseName",
        "camlCaseArgumentName",
        "optionalArgsOrder"
    ],
    "modes": {
        "project": {
            "entries": [{
                    "template": "project.hbs",
                    "naming": "project.json",
                    "selector": ""
                },
                {
                    "template": "xproj.hbs",
                    "naming": "{{options.clientName}}.xproj",
                    "selector": ""
                },
                {
                    "template": "client.hbs",
                    "naming": "{{options.clientName}}.cs",
                    "selector": ""
                },
                {
                    "template": "clientOptions.hbs",
                    "naming": "{{options.clientName}}Options.cs",
                    "selector": ""
                },
                {
                    "template": "clientExtensions.hbs",
                    "naming": "{{options.clientName}}Extensions.cs",
                    "selector": ""
                },
                {
                    "template": "definition.hbs",
                    "naming": "{{data.name}}.cs",
                    "selector": "definitions"
                }
            ]
        },
        "project-msbuild": {
            "entries": [{
                    "template": "csproj.hbs",
                    "naming": "{{options.rootNamespace}}.csproj",
                    "selector": ""
                },
                {
                    "template": "client.hbs",
                    "naming": "{{options.clientName}}.cs",
                    "selector": ""
                },
                {
                    "template": "clientOptions.hbs",
                    "naming": "{{options.clientName}}Options.cs",
                    "selector": ""
                },
                {
                    "template": "clientExtensions.hbs",
                    "naming": "{{options.clientName}}Extensions.cs",
                    "selector": ""
                },
                {
                    "template": "definition.hbs",
                    "naming": "{{data.name}}.cs",
                    "selector": "definitions"
                }
            ]
        },
        "classes-only": {
            "entries": [{
                    "template": "client.hbs",
                    "naming": "{{options.clientName}}.cs",
                    "selector": ""
                },
                {
                    "template": "clientOptions.hbs",
                    "naming": "{{options.clientName}}Options.cs",
                    "selector": ""
                },
                {
                    "template": "clientExtensions.hbs",
                    "naming": "{{options.clientName}}Extensions.cs",
                    "selector": ""
                },
                {
                    "template": "definition.hbs",
                    "naming": "{{data.name}}.cs",
                    "selector": "definitions"
                }
            ]
        }
    }
}